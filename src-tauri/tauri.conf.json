    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "album",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": true
    },
    "updater": {
      "active": true,
      "dialog": true,
      "endpoints": ["https://rui-lucky.github.io/album/install.json"],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEZENDQwRkFFQkFCNEZDMDIKUldRQy9MUzZyZzlFL2VsK3NGbTNjUW5meHdhS0tMcE9xaUFZa1YrY3Fld2hvR0tZdFArcWZHSlQK"
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
